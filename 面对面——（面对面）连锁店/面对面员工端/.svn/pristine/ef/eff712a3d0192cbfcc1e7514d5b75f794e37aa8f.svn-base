html{
	width: 100%;
	height: 100%;
}
body {
	background:#f2f2f2;
	width: 100%;
	height: 100%;
}
#header{
	background: #FFFFFF;
	color: #282828;
}
#left{
	color: #282828;
}
.titlecol{
	font-size: 0.85rem;
	font-family: "微软雅黑";
	font-weight: bold;
	text-align: center;
}
.Sub{
	color:#FDA340;
}
.normal{
	margin-top:1rem;
	height: 2.5rem;
	padding: 0 0.75rem;
	width: 100%;
	background:#fff;
	line-height:2.5rem;
	margin-bottom:0.5rem;
}
.normal:active{
	background: #fafafa;
}
.normal span{
	font-size: 0.7rem;
	font-family: "微软雅黑";
	float:left;
}
.normal i{
	float:right;
}
.big{
	height: 2.5rem;
	padding: 0 0.75rem;
	width: 100%;
	background:#fff;
	line-height:2.5rem;
	margin-bottom:0.5rem;
}
.big:active{
	background: #fafafa;
}
.big span{
	font-size: 0.85rem;
	font-family: "微软雅黑";
	float:left;
}
.big i{
	float:right;
}
.Very_big{
	height: 2.5rem;
	padding: 0 0.75rem;
	width: 100%;
	background:#fff;
	line-height:2.5rem;
	margin-bottom:0.5rem;
}
.Very_big:active{
	background: #fafafa;
}
.Very_big span{
	font-size: 1rem;
	font-family: "微软雅黑";
	float:left;
}
.Very_big i{
	float:right;
}
i{
	color: #989898;
}