<!DOCTYPE html>
  <html>
  <head>
      <meta charset="utf-8">
      <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
      <title>title</title>
      <link rel="stylesheet" type="text/css" href="../css/api.css" />
      <link rel="stylesheet" type="text/css" href="../css/aui.css" />
      <link rel="stylesheet" type="text/css" href="../css/employeeTrain_newMan.css" />

  </head>
  <body>
    <header class="aui-bar aui-bar-nav headercol" id="header">
        <div class="aui-pull-left aui-btn">
        </div>
        <div class="aui-title titlecol">面对面</div>
    </header>
      <div class="jiange"></div>

      <!-- 列表 -->
      <div class="list_frame listframe_bottom" onclick="newManTrian()">
        <span>新人培训</span>
        <span class="aui-iconfont aui-icon-right right"></span>
      </div>
      <div class="list_frame listframe_bottom" onclick="postTrian()">
        <span>岗位培训</span>
        <span class="aui-iconfont aui-icon-right right"></span>
      </div>
      <div class="list_frame" onclick="goodTrian()">
        <span>产品培训</span>
        <span class="aui-iconfont aui-icon-right right"></span>
      </div>


  </body>
  <script type="text/javascript" src="../script/api.js"></script>
  <script type="text/javascript">
      apiready = function(){
          var header=$api.byId("header");
          $api.fixStatusBar(header);
          api.addEventListener({
  						name: 'Font_normal'
  				}, function(ret, err) {
  						$('html').css("font-size","125%");
  				});
  				api.addEventListener({
  						name: 'Font_big'
  				}, function(ret, err) {
  						$('html').css("font-size","156.25%");
  				});
      };

      function  goDetail(){
        api.openWin({
            name: 'employeeTrain_details_header',
            url: './employeeTrain_details_header.html',
            pageParam: {
                name: 'test'
            }
        });
      }
      function goodTrian(){
        api.openWin({
            name: 'employeeTrain_good_header',
            url: './employeeTrain_good_header.html',
            pageParam: {
                name: 'test'
            }
        });
      }
      function newManTrian(){
        api.openWin({
            name: 'employeeTrain_newMan_header',
            url: './employeeTrain_newMan.html',
            pageParam: {
                name: 'test'
            }
        });
      }
      function postTrian(){
        api.openWin({
            name: 'employeeTrain_post',
            url: './employeeTrain_post.html',
            pageParam: {
                name: 'test'
            }
        });
      }
  </script>
  </html>
